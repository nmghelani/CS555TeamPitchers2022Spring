    <link rel="stylesheet" href="/public/css/site.css" />

    <style>

      #gameBox { position: absolute; width: 550; height: 400; background-image:
      url(public/images/dart.png); white-space: nowrap; overflow: hidden; }
      #dart { position: absolute; visibility:hidden; background-image:
      url(public/images/dart-flight.gif); left: 300; width: 57px; height: 400px;
      visible: hidden; } #scoreA { position: absolute; left: 29; top: 135;
      width: 68; height: 155; text-align: center; font-size: 15; color:
      rgb(255,255,255); font-weight: bold; overflow: hidden; } #power-back {
      position: absolute; visibility: hidden; left:510; top:130; width: 36;
      height: 150; background-image: url(public/images/pbar.gif);
      background-position: -36px 0px; } #power { position: absolute; visibility:
      hidden; left:0; top:0; width: 36; height: 150; background-image:
      url(public/images/pbar.gif); } #debug { width:300; height:600; overflow:
      auto; } #darts { position: absolute; top: 288; left: 34; width: 115;
      height: 108; background-image: url(public/images/darts.png); } #handCursor
      { position: absolute; top: 350; left: 450; width: 100; height: 114;
      background-image: url(public/images/hand-csr.gif); } #windIndicator {
      position: absolute; top: 11; left: 481; width: 58; height: 58; } .windCell
      { position: absolute; width: 9; height: 9; background-image:
      url(public/images/wind.gif); }

    </style>
  </head>
